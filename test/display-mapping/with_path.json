{
  "display": {
    "title": {
      "path": ["$.name", "$.vc.name"],
      "schema": {
        "type": "string"
      },
      "fallback": "Washington State Driver License"
    },
    "subtitle": {
      "path": ["$.issuanceDate", "$.vc.issuanceDate"],
      "schema": {
        "type": "string",
        "format": "date-time"
      },
      "fallback": "Issuance Date Unknown"
    }
  }
}